{
    "external": {
        "type": "json",
        "fileEnum": "MANIFEST__PRODUCTION"
    },
    "name": "Iris",
    "version": 1,
    "strings": [
        {
            "name": "PRODUCTION",
            "Currentstate": "STABLE"
        },
        {
            "name": "DEVELOPMENT",
            "Currentstate": "UNSTABLE"
        }
    ],
    "build": "DEVELOPMENT",
    "schema": {
        "database": [
            {
                "db-name": "MongoDB",
                "db-password": null,
                "db-ip": "127.0.0.1"
            }
        ],
        "Iris": {
            "ID": "IRIS",
            "name": "Iris",
            "version": "1.0.0",
            "developer/author": "NotSkeislol",
            "package": {
                "mainEntryPoint": "index.js",
                "scripts": {
                    "start": "node index.js",
                    "build": ""
                },
                "repository": {
                    "type": "git",
                    "url": "git+https://github.com/NotSkeislol/Iris.git"
                },
                "keywords": [
                    "Discord"
                ],
                "license": "Apache-2.0",
                "bugs": {
                    "url": "https://github.com/NotSkeislol/Iris/issues"
                },
                "homepage": "https://github.com/NotSkeislol/Iris#readme"
            }
        },
        "server": {
            "ID": "IRIS_SERVER",
            "name": "Server",
            "version": "2.0.1",
            "config": {
                "port": 3000,
                "ws": 80,
                "databaseURL": null
            }
        },
        "web": {
            "ID": "IRIS_WEB",
            "name": "Public",
            "version": "2.0.0",
            "config": {
                "main": "public/index.html",
                "bUseLiveServer": true,
                "port": 55000,
                "ws": 443,
                "layout": [
                    "public/index.html",
                    "public/register.html",
                    "public/login.html",
                    "public/group.html",
                    "public/host.html",
                    "public/create.html",
                    "public/document.txt"
                ]
            }
        },
        "mobile": {
            "ios": [
                {
                    "name": "IOS",
                    "defaultG": "IOS_DEFAULT_GATEWAY",
                    "ID": "__IOS__PRODUCTION__BUILD__",
                    "Currentstate": "UNSTABLE"
                }
            ],
            "android": [
                {
                    "name": "ANDROID",
                    "defaultG": "ANDROID_DEFAULT_GATEWAY",
                    "ID": "__ANDROID__PRODUCTION__BUILD__",
                    "Currentstate": "UNSTABLE"
                }
            ]
        },
        "windows": {
            "support": [
                "WIN_10",
                "WIN_11",
                "WIN_7"
            ]
        },
        "other": {
            "support": [
                "LNX",
                "MAC"
            ]
        },
        "frameworks": null,
        "data": "bin/Enums.js",
        "functions": "utils/functions.js",
        "logger": [
            "utils/Logger.js"
        ],
        "IRIS_SDK": {
            "version": "3.0.0",
